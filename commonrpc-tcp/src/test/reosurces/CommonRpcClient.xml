<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:commonrpc="http://www.commonrpc.com/commonrpc"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.commonrpc.com/commonrpc http://www.commonrpc.com/commonrpc/commonrpc.xsd">
    
    <!--redis 注册中心地址  -->
    <commonrpc:application id="rpcRegistryApplicationClient" address="127.0.0.1:6379,127.0.0.1:6379"></commonrpc:application>
    
    <commonrpc:reference id="demoServiceClient"  interfacename="test.cross.plateform.rocketrpc.demo.service.IDemoService"  procotolType="1" codecType="4" timeout="50000" token="1122" group="demo"/>
    
</beans>